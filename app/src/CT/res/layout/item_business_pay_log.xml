<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_padding_x_min"
        android:layout_marginTop="@dimen/dp_20"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/sk_icon" />

    <TextView
        android:id="@+id/tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_8"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/status_reving"
        android:layout_marginTop="@dimen/dp_5"
        android:textColor="@color/color_24CE94"
        android:textSize="@dimen/sp_12"
        app:layout_constraintStart_toStartOf="@+id/tv_address"
        app:layout_constraintTop_toBottomOf="@+id/tv_address" />

    <TextView
        android:id="@+id/tv_price_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_padding_x_min"
        android:text="@string/eth"
        android:textColor="@color/main_blue"
        android:textSize="@dimen/sp_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_address" />

    <TextView
        android:id="@+id/tv_coin_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/usdt"
        android:layout_marginTop="@dimen/dp_10"
        android:textColor="@color/color_text_1"
        app:layout_constraintEnd_toEndOf="@+id/tv_price_name"
        app:layout_constraintTop_toBottomOf="@+id/tv_price_name" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:text="@string/formate_time"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="@dimen/dp_10"
        android:textColor="@color/color_text_1"
        android:textSize="@dimen/sp_12"
        app:layout_constraintStart_toStartOf="@+id/tv_status"
        app:layout_constraintTop_toBottomOf="@+id/tv_status" />

    <TextView
        android:id="@+id/tv_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/_000"
        android:textColor="@color/main_blue"
        android:textSize="@dimen/sp_16"
        app:layout_constraintBottom_toBottomOf="@+id/tv_price_name"
        app:layout_constraintEnd_toStartOf="@+id/tv_price_name"
        app:layout_constraintTop_toTopOf="@+id/tv_price_name" />

    <TextView
        android:id="@+id/tv_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_5"
        android:text="@string/_000"
        android:textColor="@color/color_text_1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_coin_name"
        app:layout_constraintEnd_toStartOf="@+id/tv_coin_name"
        app:layout_constraintTop_toTopOf="@+id/tv_coin_name" />

    <TextView
        android:id="@+id/textView138"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_5"
        android:text="â‰ˆ"
        android:textColor="@color/color_text_1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_amount"
        app:layout_constraintEnd_toStartOf="@+id/tv_amount"
        app:layout_constraintTop_toTopOf="@+id/tv_amount" />
</android.support.constraint.ConstraintLayout>