<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_103"

        android:background="@mipmap/zz_top_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:padding="@dimen/dp_padding_x_min"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/back_not_header" />

        <com.qingyun.mvpretrofitrx.mvp.weight.BoldTextView

            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/asset_detail"
            android:textColor="@color/color_FFFFFF"
            android:textSize="@dimen/sp_17"
            app:layout_constraintBottom_toBottomOf="@+id/btn_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_back"
            app:layout_constraintVertical_bias="0.571" />
    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:layout_width="@dimen/dp_0"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_74"
        android:background="@drawable/bg_ffffff_top_round_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <android.support.constraint.ConstraintLayout
            android:id="@+id/ly_asset"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_200"
            android:layout_marginStart="@dimen/dp_padding_x_min"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_padding_x_min"
            android:background="@mipmap/sy_qb_bg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginEnd="@dimen/dp_10"
                android:drawableStart="@mipmap/sy_anquan"
                android:drawablePadding="@dimen/dp_5"
                android:text="@string/asset_protecting"
                android:textColor="@color/main_blue"
                android:textSize="@dimen/sp_12"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView

                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:textColor="@color/color_FFFFFF"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_40"
                android:text="@string/my_asset"
                android:textSize="@dimen/sp_13"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/btn_visiable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/textView2"
                app:layout_constraintStart_toEndOf="@+id/textView2"
                app:layout_constraintTop_toTopOf="@+id/textView2"
                app:srcCompat="@mipmap/sy_xianshi" />

            <TextView
                android:id="@+id/tv_asset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:includeFontPadding="false"
                android:textColor="@color/color_FFFFFF"
                android:text="@string/_000"
                android:textSize="@dimen/sp_32"
                app:layout_constraintStart_toStartOf="@+id/textView2"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/today"
                android:textColor="@color/color_FFFFFF"
                app:layout_constraintStart_toStartOf="@+id/tv_asset"
                app:layout_constraintTop_toBottomOf="@+id/tv_asset" />

            <TextView
                android:id="@+id/tv_income_today"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_5"
                android:text="@string/_000"
                android:textColor="@color/color_FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/textView7"
                app:layout_constraintStart_toEndOf="@+id/textView7"
                app:layout_constraintTop_toTopOf="@+id/textView7" />


        </android.support.constraint.ConstraintLayout>

        <TextView
            android:id="@+id/textView21"
            android:layout_width="@dimen/dp_120"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:text="@string/wallet_name"
            android:textColor="@color/color_text_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ly_asset" />

        <TextView
            android:id="@+id/tv_wallet_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/textView21"
            app:layout_constraintEnd_toStartOf="@+id/btn_copy_publish_key"
            app:layout_constraintStart_toEndOf="@+id/textView21"
            app:layout_constraintTop_toTopOf="@+id/textView21" />

        <TextView
            android:id="@+id/textView52"
            android:layout_width="@dimen/dp_120"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:text="@string/publish_key"
            android:layout_marginTop="@dimen/dp_35"
            android:textColor="@color/color_text_2"
            app:layout_constraintStart_toStartOf="@+id/textView21"
            app:layout_constraintTop_toBottomOf="@+id/textView21" />

        <TextView
            android:id="@+id/tv_publish_key"
            android:layout_width="0dp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dp_10"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/textView52"
            app:layout_constraintEnd_toStartOf="@+id/btn_copy_publish_key"
            app:layout_constraintStart_toEndOf="@+id/textView52"
            app:layout_constraintTop_toTopOf="@+id/textView52" />

        <TextView
            android:id="@+id/btn_change_password"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_50"
            android:layout_marginTop="@dimen/dp_20"
            android:background="@drawable/bg_item"
            android:drawableEnd="@mipmap/wd_gd_icon"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:paddingEnd="@dimen/dp_padding_x_min"
            android:text="@string/change_password"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView52" />

        <TextView
            android:id="@+id/btn_export_private_key"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_50"
            android:background="@drawable/bg_item"
            android:drawableEnd="@mipmap/wd_gd_icon"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:paddingEnd="@dimen/dp_padding_x_min"
            android:text="@string/export_privite_key"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_change_password" />

        <TextView
            android:id="@+id/btn_export_key_store"
            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:gravity="center_vertical"
            android:paddingEnd="@dimen/dp_padding_x_min"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:background="@drawable/bg_item"
            android:layout_height="@dimen/dp_50"
            android:text="@string/export_ketstore"
            android:drawableEnd="@mipmap/wd_gd_icon"
            app:layout_constraintTop_toBottomOf="@+id/btn_export_private_key"
            tools:layout_editor_absoluteX="32dp" />

        <ImageView
            android:id="@+id/btn_copy_publish_key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_publish_key"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/dp_padding_x_min"
            app:layout_constraintTop_toTopOf="@+id/tv_publish_key"
            app:srcCompat="@mipmap/zc_fuzhi" />

        <TextView
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            app:layout_constraintBottom_toTopOf="@id/textView52"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            />



        <TextView
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            app:layout_constraintBottom_toTopOf="@id/btn_change_password"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            />



        <TextView
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            app:layout_constraintBottom_toTopOf="@id/btn_export_private_key"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            />

        <TextView
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            app:layout_constraintBottom_toTopOf="@id/btn_export_key_store"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            />


        <TextView
            android:id="@+id/textView57"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:gravity="center"
            android:background="@color/color_999999"
            android:layout_height="@dimen/dp_50"
            android:text="@string/delete_wallet"
            android:textColor="@color/red"
            tools:layout_editor_absoluteX="174dp"
            tools:layout_editor_absoluteY="613dp" />


    </android.support.constraint.ConstraintLayout>








</android.support.constraint.ConstraintLayout>