<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_103"

        android:background="@mipmap/zz_top_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:padding="@dimen/dp_padding_x_min"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/back_not_header" />

        <com.qingyun.mvpretrofitrx.mvp.weight.BoldTextView

            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/shan_dui"
            android:textColor="@color/color_FFFFFF"
            android:textSize="@dimen/sp_17"
            app:layout_constraintBottom_toBottomOf="@+id/btn_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_back"
            app:layout_constraintVertical_bias="0.571" />

        <TextView
            android:id="@+id/textView84"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dp_padding_x_min"
            android:text="@string/log"
            android:textColor="@color/color_FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView8" />
    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:layout_width="@dimen/dp_0"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_74"
        android:background="@drawable/bg_ffffff_top_round_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/textView85"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_35"
            android:drawableStart="@mipmap/sd_eth"
            android:drawableEnd="@mipmap/sd_jiantou"
            android:drawablePadding="@dimen/dp_10"
            android:gravity="center_vertical"
            android:text="@string/eth"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView17"
            app:layout_constraintBottom_toBottomOf="@+id/imageView17"/>

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toStartOf="@+id/textView86"
            app:layout_constraintStart_toEndOf="@+id/textView85"
            app:srcCompat="@mipmap/sd_jian_rigjt"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/dp_30"
          />

        <TextView
            android:id="@+id/textView86"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dp_35"
            android:drawableStart="@mipmap/sd_usdt"
            android:drawableEnd="@mipmap/sd_jiantou"
            android:drawablePadding="@dimen/dp_10"
            android:gravity="center_vertical"
            android:text="@string/usdt"
            app:layout_constraintBottom_toBottomOf="@+id/textView85"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView85" />


        <TextView
            android:id="@+id/line1"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            android:layout_marginEnd="@dimen/dp_padding_x_min"
            android:layout_marginStart="@dimen/dp_padding_x_min"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView85"
            android:layout_marginTop="@dimen/dp_padding_x_min"
            />

        <EditText
            android:id="@+id/editText10"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_49"
            android:background="@color/transparent"
            app:layout_constraintTop_toBottomOf="@+id/line1"
            android:hint="@string/transfer_out_amount"
            app:layout_constraintStart_toStartOf="@+id/textView85"
            tools:layout_editor_absoluteY="117dp" />

        <EditText
            android:id="@+id/editText11"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_49"
            app:layout_constraintTop_toBottomOf="@+id/line1"
            android:hint="@string/transfer_out_amount"
            android:background="@color/transparent"
            app:layout_constraintEnd_toEndOf="@+id/textView86"
            tools:layout_editor_absoluteY="117dp" />

        <TextView
            android:id="@+id/line2"
            android:layout_width="@dimen/dp_0"
            android:layout_height="@dimen/dp_1"
            style="@style/line"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="@dimen/dp_padding_x_min"
            android:layout_marginStart="@dimen/dp_padding_x_min"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText10"

            />

        <TextView
            android:id="@+id/ben_choose_wallet"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_54"
            android:drawableEnd="@mipmap/wd_gd_icon"
            android:gravity="center_vertical"
            android:hint="@string/choose_receive_wallet"
            android:paddingStart="@dimen/dp_padding_x_min"
            android:paddingEnd="@dimen/dp_padding_x_min"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/line2" />

        <TextView
            android:id="@+id/textView88"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_padding_x_min"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/current_rate"
            android:textColor="@color/main_blue"
            android:textSize="@dimen/sp_11"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ben_choose_wallet" />

        <TextView
            android:id="@+id/textView89"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_000"
            android:textColor="@color/main_blue"
            android:textSize="@dimen/sp_11"
            app:layout_constraintBottom_toBottomOf="@+id/textView88"
            app:layout_constraintStart_toEndOf="@+id/textView88"
            app:layout_constraintTop_toTopOf="@+id/textView88" />

        <TextView
            android:id="@+id/btn_exchange"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="@dimen/dp_70"
            android:text="@string/start_exchange"
            style="@style/main_btn"
            tools:layout_editor_absoluteX="188dp"
            tools:layout_editor_absoluteY="507dp" />

        <!--<TextView-->
            <!--android:id="@+id/line3"-->
            <!--android:layout_width="@dimen/dp_0"-->
            <!--android:layout_height="@dimen/dp_1"-->
            <!--style="@style/line"-->
            <!--android:layout_marginEnd="@dimen/dp_padding_x_min"-->
            <!--android:layout_marginStart="@dimen/dp_padding_x_min"-->
            <!--app:layout_constraintStart_toStartOf="parent"-->
            <!--app:layout_constraintEnd_toEndOf="parent"-->
            <!--app:layout_constraintTop_toBottomOf="@+id/editText10"-->
            <!--android:layout_marginTop="@dimen/dp_padding_x_min"-->
            <!--/>-->

    </android.support.constraint.ConstraintLayout>








</android.support.constraint.ConstraintLayout>